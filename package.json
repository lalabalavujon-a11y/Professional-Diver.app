  "scripts": {
    "dev:api": "NODE_ENV=development PORT=5000 tsx server/index.ts",
    "dev:api:workaround": "NODE_ENV=development PORT=5000 node server/index.js",
    "dev:api:tsnode": "NODE_ENV=development PORT=5000 NODE_OPTIONS='--loader ts-node/esm' node --loader ts-node/esm server/index.ts",
    "dev:api:swc": "NODE_ENV=development PORT=5000 node --loader @swc-node/register/esm server/index.ts",
    "dev:web": "vite --host 127.0.0.1 --port 3000 --strictPort",
    "dev:all": "npm-run-all -p dev:api dev:web",
    "build": "vite build",
    "start": "CI=true npm run db:push && cross-env NODE_ENV=production HOST=0.0.0.0 tsx server/index.ts",
    "check": "tsc",
    "db:push": "yes 1 | CI=true DRIZZLE_NON_INTERACTIVE=true drizzle-kit push || true",
    "debug:start": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "debug:monitor": "node ./super-debug-agent/index.js",
    "debug:install": "node ./super-debug-agent/scripts/install.js",
    "debug:configure": "node ./super-debug-agent/scripts/configure.js",
    "dev:reset": "bash -lc 'kill -9 $(lsof -t -iTCP:3000 -sTCP:LISTEN) 2>/dev/null || true; kill -9 $(lsof -t -iTCP:5000 -sTCP:LISTEN) 2>/dev/null || true; npm run dev:all'",
    "typecheck": "tsc --noEmit",
    "init-laura-oracle": "tsx scripts/init-laura-oracle.ts",
    "laura-oracle:test": "tsx scripts/init-laura-oracle.ts",
    "import-exam-questions": "tsx scripts/import-exam-questions.ts",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "cap:build": "npm run build && cap sync",
    "mobile:build": "npm run build && cap sync",
    "mobile:ios": "npm run build && cap sync && cap open ios",
    "mobile:android": "npm run build && cap sync && cap open android"
  },
