{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev:api": "NODE_ENV=development PORT=5000 node --import tsx/esm server/index.ts",
    "dev:web": "vite",
    "dev:all": "npm-run-all -p dev:api dev:web",
    "build": "vite build",
    "build:worker": "npm run build && tsx scripts/build-worker.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate:media": "tsx scripts/apply-media-migration.ts",
    "db:migrate:media:d1": "./scripts/apply-d1-migration.sh",
    "db:generate": "drizzle-kit generate",
    "debug:start": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "debug:monitor": "node ./super-debug-agent/index.js",
    "debug:install": "node ./super-debug-agent/scripts/install.js",
    "debug:configure": "node ./super-debug-agent/scripts/configure.js",
    "dev:reset": "bash -lc 'kill -9 $(lsof -t -iTCP:3000 -sTCP:LISTEN) 2>/dev/null || true; kill -9 $(lsof -t -iTCP:5000 -sTCP:LISTEN) 2>/dev/null || true; npm run dev:all'",
    "typecheck": "tsc --noEmit",
    "init-laura-oracle": "tsx scripts/init-laura-oracle.ts",
    "laura-oracle:test": "tsx scripts/init-laura-oracle.ts",
    "predeploy": "npm run backup:export",
    "deploy": "wrangler deploy",
    "deploy:dev": "npm run backup:export && wrangler deploy --env development",
    "deploy:prod": "npm run backup:export && wrangler deploy --env production",
    "deploy:api": "wrangler deploy --config wrangler-api.toml --env production",
    "deploy:all": "npm run backup:export && pnpm run deploy:api && pnpm run deploy:prod",
    "postdeploy": "echo '⚠️  Remember to run: npm run backup:restore on the deployed environment'",
    "dns:check": "node --import tsx/esm scripts/check-dns-records.ts",
    "dns:configure": "node --import tsx/esm scripts/configure-dns-api.ts",
    "affiliate:create-admins": "tsx scripts/create-affiliate-accounts.ts",
    "email-campaigns:follow-up": "tsx scripts/send-email-campaigns.ts --follow-up",
    "email-campaigns:testimonial": "tsx scripts/send-email-campaigns.ts --testimonial",
    "email-campaigns:all": "tsx scripts/send-email-campaigns.ts",
    "email-campaigns:test": "tsx scripts/test-email-campaigns.ts",
    "email-campaigns:setup-cron": "bash cron-setup.sh",
    "test:emails": "tsx scripts/send-test-emails.ts",
    "backup:export": "tsx scripts/export-tracks-lessons.ts",
    "backup:restore": "tsx scripts/restore-tracks-lessons.ts",
    "backup:full": "tsx scripts/backup-production-database.ts",
    "deploy:verify": "tsx scripts/verify-deployment-ready.ts",
    "deploy:safe": "bash scripts/safe-deploy.sh",
    "deploy:code-only": "bash scripts/deploy-code-only.sh",
    "deploy:quizzes-safe": "tsx scripts/safe-deploy-quizzes.ts",
    "deploy:all-safe": "bash scripts/deploy-all-safe.sh",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android",
    "cap:build:ios": "pnpm run build && npx cap sync ios",
    "cap:build:android": "pnpm run build && npx cap sync android"
  },
